  <make>messageutils.pdu_debug($meta_only, $display)</make>

    <param>
        <name>Metadata Only</name>
        <key>meta_only</key>
        <value>True</value>
        <type>bool</type>
		    <option>
			    <name>True</name>
			    <key>True</key>
		    </option>
		    <option>
			    <name>False</name>
			    <key>False</key>
		    </option>
    </param>

    <param>
        <name>Display</name>
        <key>display</key>
        <value>True</value>
        <type>bool</type>
		    <option>
			    <name>On</name>
			    <key>True</key>
		    </option>
		    <option>
			    <name>Off</name>
			    <key>False</key>
		    </option>
    </param>

	<sink>
		<name>pdus</name>
		<type>message</type>
        <optional>1</optional>
	</sink>
    
