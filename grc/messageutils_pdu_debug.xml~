<?xml version="1.0"?>
<block>
  <name>PDU Debug</name>
  <key>messageutils_pdu_debug</key>
  <category>Message Utils</category>
  <import>import messageutils</import>
  <make>messageutils.pdu_debug($vec_type.size, $meta_only, $display)</make>


    <param>
      <name>Vector Type</name>
      <key>vec_type</key>
      <type>enum</type>

	    <option>
		    <name>ASCII</name>
		    <key>text</key>
        <opt>ascii:True</opt>
		    <opt>size:gr.sizeof_char</opt>
	    </option>
	    <option>
		    <name>Byte</name>
		    <key>byte</key>
        <opt>ascii:False</opt>
		    <opt>size:gr.sizeof_char</opt>
	    </option>
	    <option>
		    <name>Float</name>
		    <key>float</key>
        <opt>ascii:False</opt>
		    <opt>size:gr.sizeof_float</opt>
	    </option>
	    <option>
		    <name>Complex</name>
		    <key>complex</key>
        <opt>ascii:False</opt>
		    <opt>size:gr.sizeof_gr_complex</opt>
	    </option>
    </param>


    <param>
        <name>Metadata Only</name>
        <key>meta_only</key>
        <value>True</value>
        <type>bool</type>
		    <option>
			    <name>True</name>
			    <key>True</key>
		    </option>
		    <option>
			    <name>False</name>
			    <key>False</key>
		    </option>
    </param>

    <param>
        <name>Display</name>
        <key>display</key>
        <value>True</value>
        <type>bool</type>
		    <option>
			    <name>On</name>
			    <key>True</key>
		    </option>
		    <option>
			    <name>Off</name>
			    <key>False</key>
		    </option>
    </param>

	<sink>
		<name>pdus</name>
		<type>message</type>
        <optional>1</optional>
	</sink>
    
</block>
