<?xml version="1.0"?>
<block>
  <name>PDU to File</name>
  <key>messageutils_pdu_to_file</key>
  <category>[Utils]</category>
  <import>import messageutils</import>
  <make>messageutils.pdu_to_file($file, $append)</make>
 
 	<param>
		<name>File</name>
		<key>file</key>
		<value></value>
		<type>file_save</type>
	</param>
	
	<param>
		<name>Append file</name>
		<key>append</key>
		<value>False</value>
		<type>bool</type>
		<option>
			<name>Append</name>
			<key>True</key>
		</option>
		<option>
			<name>Overwrite</name>
			<key>False</key>
		</option>
	</param>
	
	<sink>
		<name>in</name>
		<type>message</type>
	</sink>
	
</block>
